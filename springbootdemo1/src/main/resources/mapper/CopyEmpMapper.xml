<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caihao.springbootdemo.mapper.CopyEmpMapper">

  <select id="selectCopyEmpList" resultType="com.caihao.springbootdemo.entity.CopyEmp">
    SELECT
      empno,
      ename,
      job,
      mgr,
      hiredate,
      sal,
      comm,
      deptno
    FROM copy_emp
  </select>
</mapper>